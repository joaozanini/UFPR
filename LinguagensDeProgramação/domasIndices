#include <stdio.h>

int main() {
    int matriz[3][4];

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 4; j++) {
            scanf("%d", &matriz[i][j]);
        }
    }

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 4; j++) {
            if ((i + j) % 2 == 0) {
                printf("%d\n", matriz[i][j]);
            }
        }
    }

    return 0;
}
